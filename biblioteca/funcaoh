int soma (int a, int b)
{
	return (a+b);
}
int sub (int a, int b)
{
	return (a-b);
}
int div (int a, int b)
{
	return (a/b);
}
int mult (int a, int b)
{
	return (a*b);
}
float soma (float a, float b)
{
	return (a+b);
}
float sub (float a, float b)
{
	return (a-b);
}
float div (float a, float b)
{
	return (a/b);
}
float mult (float a, float b)
{
	return (a*b);
}
